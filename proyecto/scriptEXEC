#!/bin/bash

if [ $# -ne 1 ]
then
        echo "Haz el favor de s√≥lo especificar un directorio"
        exit 1;
fi

if [ $1 == "" ]
then
    echo "no se especifico nombre del archivo"
    exit 1;
fi

if [ ! -f $1 ]
   then
    echo "archivo no encontrado"
    exit 1;
fi

./scriptCOMP
./a.out $1
gcc assembler.s -o asseblerCompilado.out 
 ./asseblerCompilado.out 
